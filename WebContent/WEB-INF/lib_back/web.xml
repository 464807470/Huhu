<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LittleSemester2</display-name>
  <welcome-file-list>
    <welcome-file>/web.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>addUserLabel</servlet-name>
    <servlet-class>com.interest.AddUserLabelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>selectUserLabel</servlet-name>
    <servlet-class>com.interest.SelectUserLabelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>addQuestLabel</servlet-name>
    <servlet-class>com.interest.AddQuestLabelServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>selectQuestLabel</servlet-name>
    <servlet-class>com.interest.SelectQuestLabelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addUserLabel</servlet-name>
    <url-pattern>/addUserLabel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>selectUserLabel</servlet-name>
    <url-pattern>/selectUserLabel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addQuestLabel</servlet-name>
    <url-pattern>/addQuestLabel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>selectQuestLabel</servlet-name>
    <url-pattern>/selectQuestLabel</url-pattern>
  </servlet-mapping>
</web-app>